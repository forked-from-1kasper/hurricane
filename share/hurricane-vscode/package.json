{
    "name": "hurricane",
    "displayName": "Hurricane",
    "version": "0.0.0",
    "description": "Hurricane theorem prover syntax highlighting for Visual Studio Code",
    "publisher": "siegmentationfault",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/groupoid/hurricane"
    },
    "engines": {
        "vscode": "^0.10.10"
    },
    "contributes": {
        "languages": [{
            "id": "hurricane",
            "extensions": [
                ".hurricane"
            ],
            "configuration": "./hurricane-configuration.json"
        }],
        "grammars": [{
                "language": "hurricane",
                "scopeName": "source.hurricane",
                "path": "./syntaxes/hurricane.tmLanguage.json"
        }]
    }
}
